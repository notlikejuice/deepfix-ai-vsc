{
	"compilerOptions": {
		"outDir": "./out",
		"rootDir": "./src",
		"module": "commonjs",
		"target": "es6",
		"lib": [
			"es6"
		],
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
		"node_modules",
		"src/**/*.js",
		"src/**/*.js.map"
	]
}